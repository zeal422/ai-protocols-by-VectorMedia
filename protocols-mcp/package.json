{
  "name": "@ai-protocols/mcp-server",
  "version": "2.3.2",
  "description": "MCP server for ai-protocols - exposes protocols, configs, and examples",
  "type": "module",
  "bin": {
    "ai-protocols-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"try { require('fs').chmodSync('build/index.js', '755'); } catch(e) { require('fs').chmodSync('build/index.js', '0o755'); }\"",
    "dev": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "keywords": ["mcp", "ai", "protocols", "development", "cursor", "claude"],
  "author": "VectorMedia",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "zod": "^3.22.4",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/js-yaml": "^4.0.5",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  }
}
